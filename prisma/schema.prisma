// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Exemplo de modelo
model jogos {
  id        Int      @id @default(autoincrement())
  nome     String   @unique
  jogo      String
  raridade String
  preco  String
  categoria String
  dataLancamento Float
  popularidade String
  disponivel String


  @@map("jogos")
}